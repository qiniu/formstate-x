{
  "name": "formstate-x",
  "version": "2.0.1",
  "description": "Extended alternative for formstate",
  "repository": {
    "type": "git",
    "url": "https://github.com/qiniu/formstate-x.git"
  },
  "main": "lib/index.js",
  "module": "esm/index.js",
  "scripts": {
    "clean": "rm -rf ./esm ./lib ./docs",
    "lint": "tsc --noEmit",
    "test": "jest",
    "validate": "npm run lint && npm run test",
    "coverage": "rm -rf coverage && jest --coverage",
    "build:esm": "rm -rf ./esm && tsc --module es6 --outDir ./esm",
    "build:lib": "rm -rf ./lib && tsc",
    "build:doc": "rm -rf ./docs && typedoc && cp -r ./assets ./docs",
    "build": "npm run build:esm && npm run build:lib",
    "prepublishOnly": "npm run test && npm run build",
    "ci": "npm run lint && npm run coverage && npm run build && npm run build:doc"
  },
  "keywords": [
    "form",
    "state",
    "mobx",
    "formstate"
  ],
  "peerDependencies": {
    "mobx": ">=6.0.0"
  },
  "author": "nighca@live.cn",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/node": "^12.7.12",
    "formstate": "^2.0.0",
    "jest": "~26.6.1",
    "mobx": "^6.0.4",
    "ts-jest": "~26.4.2",
    "typedoc": "~0.21.6",
    "typedoc-twilio-theme": "^1.0.0",
    "typescript": "^4.1.5"
  },
  "dependencies": {},
  "engines": {
    "node": ">=12.0.0"
  }
}
